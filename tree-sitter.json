{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "scss",
      "camelcase": "SCSS",
      "title": "SCSS",
      "scope": "source.css.scss",
      "file-types": [
        "scss"
      ],
      "injection-regex": "^scss$",
      "class-name": "TreeSitterScss"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "SCSS, the CSS extension language",
    "authors": [
      {
        "name": "Simeon Simeonoff",
        "email": "hello@simeonoff.ninja",
        "url": "https://simeonoff.ninja/"
      }
    ],
    "links": {
      "repository": "https://github.com/simeonoff/tree-sitter-scss/"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
